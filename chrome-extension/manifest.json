{
    "name": "PhishNet",
    "version": "0.0.1",
    "description": "Starterhacks 2020 Project",
    "background": {
        "scripts": ["background.js"]
    },
    "permissions": [
        "activeTab"
    ],
    "content_scripts": [
        {
            "js": ["content.js"],
            "css": ["content.css"]

        }
    ],
    "browser_action": {
        "default_popup": "popup.html"
    },
    "manifest_version": 2,
    "icons": {
        "16": "media/icon.png",
        "48": "media/icon.png",
        "128": "media/icon.png"
    }
}